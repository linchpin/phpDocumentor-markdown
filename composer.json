{
  "name": "linchpin/phpdocumentor-markdown-template",
  "description": "Markdown template for phpDocumentor3 based on Saggre/phpDocumentor-markdown",
  "authors": [
    {
      "name": "Aaron Ware",
      "email": "aaron@linchpin.com"
    },
    {
      "name": "Saggre",
      "email": "sakri.koskimies@hotmail.com"
    }
  ],
  "version": "0.1.5",
  "type": "library",
  "keywords": [
    "phpdocumentor",
    "phpdoc",
    "markdown",
    "github",
    "gitlab",
    "documentation"
  ],
  "require-dev": {
    "php": ">=7.4",
    "phpunit/phpunit": "^9.5",
    "twig/twig": "2.*"
  },
  "autoload": {
    "psr-4": {
      "PhpDocumentorMarkdownTemplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDocumentorMarkdownTemplate\\Test\\": "tests/"
    }
  },
  "scripts": {
    "run-tests": "vendor/bin/phpunit",
    "create-docs": "phpdoc && cp -r example/* docs/.wiki"
  },
  "license": "MIT"
}
